{
  "name": "Ara.cat noticies lluires",
  "description": "Ara.cat extension to be able to read the news",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://www.ara.cat/*"]
    }
